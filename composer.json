{
    "name": "yevhenii/privatbank",
    "description": "laravel API for working with PrivatBank API",
    "license": "MIT",
    "authors": [
        {
            "name": "Yevhenii Riabyi",
            "email": "author email"
        }
    ],
    "homepage": "https://github.com/zenia9012/privatbank",
    "keywords": ["Laravel", "PrivatBank"],
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Yevhenii\\PrivatBank\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yevhenii\\PrivatBank\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yevhenii\\PrivatBank\\PrivatBankServiceProvider"
            ],
            "aliases": {
                "PrivatBank": "Yevhenii\\PrivatBank\\Facades\\PrivatBank"
            }
        }
    }
}
